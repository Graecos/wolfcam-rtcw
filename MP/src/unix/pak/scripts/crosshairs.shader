// added _w extension as in _OSP since these didn't seem to override the
// original ones

gfx/2d/crosshaira_w
{
	nopicmip
	nocompress
	{
		map gfx/2d/crosshaira.tga
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen vertex
	}
}

